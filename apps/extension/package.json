{
  "name": "@sokolia/extension",
  "scripts": {
    "start": "ts-node-esm ./scripts/start.ts",
    "build": "ts-node-esm ./scripts/build.ts",
    "clean": "rimraf dist",
    "build:v2": "tsc --noEmit && vite build --config vite/v2.config.ts",
    "build:v3": "tsc --noEmit && vite build --config vite/v3.config.ts",
    "dev:v2": "vite build --watch --config vite/v2.config.ts",
    "dev:v3": "vite build --watch --config vite/v3.config.ts"
  },
  "type": "module",
  "dependencies": {
    "date-fns": "^2.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.8.0",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.209",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/webextension-polyfill": "^0.10.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.15",
    "concurrently": "^7.6.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "^11.1.1",
    "get-installed-browsers": "^0.1.7",
    "postcss": "^8.4.29",
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.4.9",
    "web-ext": "^7.7.0"
  }
}